    "lint": "eslint ./src"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "require": [
      "babel-polyfill",
      "babel-register"
    ],
    "babel": "inherit",
    "verbose": true
  }
